<%= form_for @entry, html: { multipart: true } do |form| %>
  <p><%= form.file_field :image %></p>
  <p><%= form.label("title") %></p>
  <p><%= form.text_field("youtube_url") %></p>
  <p><%= form.text_field("title") %></p>
  <p><%= form.label("date") %></p>
  <p><%= form.date_field("date") %></p>
  <p><%= form.label("contents") %></p>
  <p><%= form.text_area("contents") %></p>
  <p><%= form.submit %></p>
<% end %>
